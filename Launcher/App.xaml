<Application x:Class="Launcher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Launcher"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
	        <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style/GlobalStyle.xaml"/>
                <ResourceDictionary Source="Style/ButtonStyle.xaml"/>
                <ResourceDictionary Source="Style/ScrollBarThumbStyle.xaml"/>
                <ResourceDictionary Source="Style/ScrollBarRepeatButtonStyle.xaml"/>
                <ResourceDictionary Source="Style/HorizontalScrollBarStyle.xaml"/>
                <ResourceDictionary Source="Style/VerticalScrollBarStyle.xaml"/>
                <ResourceDictionary Source="Style/ScrollBarStyle.xaml"/>
                <ResourceDictionary Source="Style/ScrollViewerStyle.xaml"/>
                <ResourceDictionary Source="Style/LevelButtonStyle.xaml"/>
	        </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
